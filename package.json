{
  "name": "smarthr-patterns",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "run-s storybook",
    "storybook": "storybook dev -p 8008",
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider storybook build",
    "lint": "run-p lint:*",
    "lint:js": "eslint './**/*.ts{,x}'",
    "lint:css": "echo \"後でちゃんとして! stylelint './**/*.ts{,x}'\"",
    "format": "run-p format:*",
    "format:js": "run-s 'lint:js --fix'",
    "format:css": "echo \"後でちゃんとして! runs-s 'lint:css --fix\""
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "smarthr-normalize-css": "^1.1.0",
    "smarthr-ui": "^42.0.0",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@storybook/addon-actions": "^8.5.0",
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/addon-interactions": "^8.5.1",
    "@storybook/addon-links": "^8.5.1",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/react": "^8.5.1",
    "@storybook/react-vite": "^8.5.1",
    "@storybook/testing-library": "^0.2.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "eslint": "^8.57.0",
    "eslint-config-smarthr": "^7.3.0",
    "eslint-plugin-storybook": "^0.11.3",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.2",
    "prettier-config-smarthr": "^1.0.1",
    "storybook": "^8.5.1",
    "stylelint-config-smarthr": "^3.0.2",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite": "^5.4.14",
    "vite-plugin-turbosnap": "^1.0.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "resolutions": {
    "jackspeak": "2.3.6"
  }
}
